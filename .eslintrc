{
    "extends": [
      "react-app",
      "airbnb",
      "plugin:jsx-a11y/recommended",
      "prettier"
    ],
    "plugins": [
      "react",
      "jsx-a11y",
      "prettier"
    ],
    "rules": {
      "no-plusplus": 0,
      "jsx-a11y/click-events-have-key-events": 0, 
      "jsx-a11y/no-noninteractive-element-interactions": 0, 
      "consistent-return": 0,
      "react/require-default-props": 0,
      "semi": 0,
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "prettier/prettier": [
        "error", {
          "semi": false
        }
      ]
    }
  }